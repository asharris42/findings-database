<% provide(:title, 'Activity Types') %>

<ul class="breadcrumb">
	<li><%= link_to "Admin Panel", admin_panel_path %></li>
	<li>Activity Types</li>
</ul>

<h1>Activity Types</h1>

<% if signed_in? %>
	<div class="btn-right">
		<a href="<%= new_activity_type_path %>" class="symphony">
  		<div class="btn-bg">
    		<b class="icon add-user"></b>
    		<span>New Activity Type</span>
  		</div>
		</a>
	</div>
<% end %>

<div class="table-data">
	<div class="table-data-content">
  	<%= render @activity_types %>
	</div>
</div>